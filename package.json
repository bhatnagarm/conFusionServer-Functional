{
	"name": "confusionserver",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node ./bin/www"
	},
	"dependencies": {
		"clean-css": "^4.2.1",
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.9",
		"express": "~4.16.0",
		"fastify": "^1.13.3",
		"fastify-sensible": "^1.0.0",
		"http-errors": "~1.6.2",
		"jade": "~1.11.0",
		"mongoose": "^5.5.11",
		"mongoose-currency": "^0.2.0",
		"morgan": "~1.9.0",
		"prettier": "^1.15.3",
		"xo": "^0.23.0"
	},
	"xo": {
		"globals": [
			"$",
			"_",
			"dbError",
			"config"
		],
		"ignores": [
			"python_modules/"
		],
		"prettier": true,
		"rules": {
			"camelcase": "off",
			"generator-star-spacing": "off",
			"import/no-extraneous-dependencies": "off",
			"import/no-unassigned-import": "off",
			"import/no-unresolved": "off",
			"import/order": "off",
			"operator-linebreak": "off"
		},
		"semicolon": false
	}
}
